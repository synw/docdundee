{
  "name": "@docdundee/vue",
  "version": "0.0.12",
  "description": "Vuejs components for Docdundee",
  "repository": "https://github.com/synw/docdundee",
  "scripts": {
    "watch": "vite build --watch",
    "compile": "vite build",
    "emit": "vue-tsc --declaration --emitDeclarationOnly",
    "css": "npx tailwindcss build -i ./dist/style.css -o ./dist/twstyle.css --minify",
    "cpcss": "rm -f ./dist/style.css && mv ./dist/twstyle.css ./dist/style.css && rm -f ./dist/twstyle.css",
    "build": "run-s compile emit css cpcss"
  },
  "dependencies": {
    "@docdundee/nav": "^0.0.9",
    "highlight.js": "^11.6.0",
    "markdown-it": "^13.0.1",
    "vuepython": "^0.0.15"
  },
  "peerDependencies": {
    "vue": "^3.2.31"
  },
  "devDependencies": {
    "@snowind/plugin": "0.4.0",
    "@tailwindcss/forms": "^0.5.0",
    "@tailwindcss/typography": "^0.5.9",
    "@types/highlight.js": "^10.1.0",
    "@types/markdown-it": "^12.2.3",
    "@types/node": "^16.10.2",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/compiler-sfc": "^3.2.37",
    "autoprefixer": "^10.4.4",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "postcss": "^8.4.12",
    "sass": "^1.50.0",
    "tailwindcss": "^3.0.23",
    "tailwindcss-semantic-colors": "^0.2.0",
    "tslib": "^2.3.1",
    "typescript": "^4.6.3",
    "vite": "^4.0.4",
    "vue": "^3.2.31",
    "vue-docgen-cli": "^4.61.2",
    "vue-tsc": "^0.40.5"
  },
  "files": [
    "dist"
  ],
  "style": "./dist/style.css",
  "main": "./dist/vue.umd.js",
  "module": "./dist/vue.mjs",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "import": "./dist/vue.mjs"
    },
    "./style.css": "./dist/style.css"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "license": "MIT"
}