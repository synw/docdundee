{
  "name": "restmix",
  "version": "0.0.7",
  "description": "A lightweight requests manager for rest apis",
  "repository": "https://github.com/synw/restmix",
  "scripts": {
    "build": "rm -f dist/* && rollup -c",
    "test": "jest --coverage",
    "docs": "typedoc --entryPointStrategy expand"
  },
  "dependencies": {
    "js-cookie": "^3.0.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.3.1",
    "@types/expect": "^24.3.0",
    "@types/jest": "^29.1.2",
    "@types/js-cookie": "^3.0.1",
    "@types/node": "^16.10.2",
    "jest": "^29.1.2",
    "markdown-it-replace-link": "^1.1.0",
    "rollup": "^2.70.0",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.3.1",
    "typedoc": "^0.23.5",
    "typedoc-plugin-markdown": "^3.13.2",
    "typedoc-plugin-rename-defaults": "^0.6.1",
    "typescript": "^4.5.5"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/api.cjs.min.js",
  "module": "./dist/api.es.js",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "import": "./dist/api.es.js",
      "require": "./dist/api.cjs.min.js"
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "license": "MIT"
}